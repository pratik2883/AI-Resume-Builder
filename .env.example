# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/resumebuilder

# Session Configuration
SESSION_SECRET=your-secret-key

# SSL Configuration
# Set NODE_ENV to 'production' to enable SSL
# NODE_ENV=production
SSL_DOMAINS=example.com,www.example.com
SSL_EMAIL=your-email@example.com
# Set to 'true' to use Let's Encrypt staging (for testing)
SSL_STAGING=false

# OpenRouter API Key (for AI content generation)
OPENROUTER_API_KEY=your-api-key